<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @project.name %>
</p>

<p>
  <b>Description:</b>
  <%= @project.description %>
</p>
<p>
	<b>Tags:</b>
	<ul>
	<% @project.tags.each do |tag| %>
